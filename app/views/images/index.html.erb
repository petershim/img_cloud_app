<div style="text-align: center">
  <h1>Frontpage</h2>
  <div class="row">
    <% @images.each do |image| %>
      <div class= <%=image.popularity%> >
        <a href="/images/<%=image.id%>" class="thumbnail">
          <img src="<%=image.url%>" alt="...">
        </a>
        <div class="caption">
          <h3><%=image.title%></h3><p> By <%=image.user.name%></p>
          <p>Upvotes: <%=image.upvotes%> <%= link_to "", upvotes_path(:id => image.id), class: "glyphicon glyphicon-circle-arrow-up" %></p>
        </div>
      </div>
    <%end%>
  </div>
</div>
